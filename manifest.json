{
    "manifest_version": 2,
    "name": "Voat Enhancement",
    "version": "1.0.0",
    "description": "Voat enhancement",
    "content_scripts": [
        {
            "matches": [
                "https://voat.co/*"
            ],
            "js": [
                "voat.js"
            ],
            "css": [
                "voat.css"
            ]
        }
    ],
    "permissions": [
        "https://*/*"
    ]
}
