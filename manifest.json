{
    "manifest_version": 2,
    "name": "Vee",
    "version": "1.2.1",
    "description": "Vee",
    "content_scripts": [
        {
            "matches": [
                "https://voat.co/*"
            ],
            "js": [
                "jquery.js",
                "vee.js"
            ],
            "css": [
                "vee.css"
            ]
        }
    ],
    "permissions": [
        "http://voat.co/*", "https://voat.co/*"
    ],
    "icons": {
        "128": "icon_128.png",
        "48": "icon_48.png",
        "32": "icon_32.png",
        "16": "icon_16.png"
    }
}
